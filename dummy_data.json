  "vehicle_registration_graphql": {
    "query": "mutation RegisterVehicle($input: VehicleInput!) { registerVehicle(input: $input) { vehicle_id make model plate_number color year } }",
    "variables": {
      "input": {
        "driver_id": 2,
        "make": "Toyota",
        "model": "Camry",
        "plate_number": "DL01AB1234",
        "color": "White",
        "year": 2020
      }
    }
  },

  "rating_add_graphql": {
    "query": "mutation AddRating($input: RatingInput!) { addRating(input: $input) { rating_id ride_id score comment } }",
    "variables": {
      "input": {
        "ride_id": 1,
        "given_by": 1,
        "given_to": 2,
        "score": 5,
        "comment": "Excellent service!"
      }
    }
  },

  "vehicle_query_graphql": {
    "query": "query GetVehicleByDriver($driver_id: Int!) { getVehicleByDriver(driver_id: $driver_id) { vehicle_id make model plate_number color year } }",
    "variables": {
      "driver_id": 2
    }
  },

  "ratings_query_graphql": {
    "query": "query GetRatingsByUser($user_id: Int!) { getRatingsByUser(user_id: $user_id) { rating_id ride_id given_by score comment } }",
    "variables": {
      "user_id": 2
    }
  },

  "vehicle_update_graphql": {
    "query": "mutation UpdateVehicle($driver_id: Int!, $input: VehicleUpdateInput!) { updateVehicle(driver_id: $driver_id, input: $input) { vehicle_id make model plate_number color year } }",
    "variables": {
      "driver_id": 2,
      "input": {
        "color": "Black",
        "year": 2021
      }
    }
  },

  "vehicle_delete_graphql": {
    "query": "mutation DeleteVehicle($driver_id: Int!) { deleteVehicle(driver_id: $driver_id) }",
    "variables": {
      "driver_id": 2
    }
  },

  "soap_payment_xml": "<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n<soap:Envelope xmlns:soap=\"http://schemas.xmlsoap.org/soap/envelope/\" xmlns:tns=\"http://localhost:3000/soap/payment\">\n  <soap:Body>\n    <tns:createPaymentRequest>\n      <tns:ride_id>1</tns:ride_id>\n      <tns:amount>150.00</tns:amount>\n      <tns:method>credit_card</tns:method>\n    </tns:createPaymentRequest>\n  </soap:Body>\n</soap:Envelope>",

