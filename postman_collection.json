              "raw": "{\n  \"query\": \"mutation RegisterVehicle($input: VehicleInput!) { registerVehicle(input: $input) { vehicle_id make model plate_number color year } }\",\n  \"variables\": {\n    \"input\": {\n      \"driver_id\": 2,\n      \"make\": \"Toyota\",\n      \"model\": \"Camry\",\n      \"plate_number\": \"DL01AB1234\",\n      \"color\": \"White\",\n      \"year\": 2020\n    }\n  }\n}"
            },
            "url": {
              "raw": "http://localhost:3000/graphql",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3000",
              "path": ["graphql"]
            }
          }
        },
